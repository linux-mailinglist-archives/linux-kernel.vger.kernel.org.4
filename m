  > 69			if (!strncmp(bootloader, (char __user *)kimage->segment[i].buf, strlen(bootloader))) {
    70				if (!copy_from_user(cmdline_ptr, kimage->segment[i].buf, COMMAND_LINE_SIZE))
    71					kimage->arch.cmdline_ptr = (unsigned long)cmdline_ptr;
    72				break;
    73			}
    74		}
    75	
    76		if (!kimage->arch.cmdline_ptr) {
    77			pr_err("Command line not included in the provided image\n");
    78			return -EINVAL;
    79		}
    80	
